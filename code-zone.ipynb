6) a['source_zip'] = a['source_zip'].apply(str)
7) a['destination_zip'] = a['destination_zip'].apply(str)



8) a['szone'] = pd.Series(np.random.randn(len(a['carrier'])),index = a.index)
9) a['dzone'] = pd.Series(np.random.randn(len(a['carrier'])),index = a.index)



10) a.reset_index(drop=True, inplace=True)
11) for x in range(len(a)):
    alist = a['source_zip'][x]
    if (alist)[0] in '12':
        a['szone'][x]='north'
    elif (alist)[0] in '34':
        a['szone'][x]='west'
    elif (alist)[0] in '56':
        a['szone'][x]='south'
    elif (alist)[0] in '78':
        a['szone'][x]='east'
    else:
        a['szone'][x]='army'



  
13) for x in range(len(a)):
